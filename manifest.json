{
    "manifest_version": 3,
    "name": "Shopping Domain Checker",
    "version": "1.0",
    "description": "Check if a domain is a shopping site using Google search results.",
    "permissions": [
      "tabs",
      "scripting"
    ],
    "host_permissions": [
      "https://www.google.com/*"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  